{
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended" // has to be last configuration
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-console": 1,
      "prettier/prettier": 2
    }

  }