{
  "name": "reference",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:coverage": "rm -rf coverage",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepush": "git diff HEAD --quiet && npm run lint && npm test",
    "test": "npm run clean:coverage && jest --coverage",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "build": "tsc --project tsconfig.extended.build.json",
    "start": "nodemon",
    "inspect": "nodemon --inspect src/server/server.ts",
    "serve": "npm run build && node dist/server.js"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^5.2.3",
    "express-slow-down": "^1.3.1",
    "helmet": "^4.2.0",
    "http-errors": "^1.7.2",
    "i18n": "^0.13.2",
    "pm2": "^4.5.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-slow-down": "^1.3.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/i18n": "^0.8.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
